                <div class="col-lg-12">
                    <h1>TEMPLATE MASSA!!!</h1>
                    <p>ESSA É A TELA DE DICAS</p>
                </div>
            </div>
        </div>
    </section>

    <!-- historico Section -->
    <section id="historico" class="historico-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h1>TEMPLATE MASSA!!!</h1>
                    <p>ESSA É A TELA DE HISTÓRICO - FEATURE</p>
                </div>
            </div>
        </div>
    </section>
    
    <!-- Contact Section -->
    <section id="comprar" class="comprar-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h1>TEMPLATE MASSA!!!</h1>
